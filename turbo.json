{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env"
  ],
  "globalEnv": [
    "NEXT_PUBLIC_*",
    "POSTGRES_URL",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL_NO_SSL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_USER",
    "POSTGRES_HOST",
    "POSTGRES_PASSWORD",
    "POSTGRES_DATABASE",
    "NEXTAUTH_URL",
    "NEXT_PUBLIC_ROOT_DOMAIN",
    "BLOB_READ_WRITE_TOKEN",
    "NEXTAUTH_SECRET",
    "AUTH_GITHUB_ID",
    "AUTH_GITHUB_SECRET",
    "AUTH_BEARER_TOKEN",
    "PROJECT_ID_VERCEL",
    "TEAM_ID_VERCEL",
    "OPENAI_API_KEY",
    "NEXT_PUBLIC_APP_NAME",
    "NEXT_PUBLIC_APP_SHORT_DOMAIN",
    "NEXT_PUBLIC_APP_DOMAIN",
    "NEXT_PUBLIC_VERCEL_ENV",
    "NEXT_PUBLIC_NGROK_URL",
    "NEXT_PUBLIC_IS_DUB",
    "DATABASE_URL",
    "NEXT_PUBLIC_ROOT_DOMAIN",
    "NOTION_API_KEY",
    "VERCEL_ENV",
    "VERCEL_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "build/**",
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "NEXT_PUBLIC_*",
        "POSTGRES_URL",
        "POSTGRES_PRISMA_URL",
        "POSTGRES_URL_NO_SSL",
        "POSTGRES_URL_NON_POOLING",
        "POSTGRES_USER",
        "POSTGRES_HOST",
        "POSTGRES_PASSWORD",
        "POSTGRES_DATABASE",
        "NEXTAUTH_URL",
        "NEXT_PUBLIC_ROOT_DOMAIN",
        "BLOB_READ_WRITE_TOKEN",
        "NEXTAUTH_SECRET",
        "AUTH_GITHUB_ID",
        "AUTH_GITHUB_SECRET",
        "AUTH_BEARER_TOKEN",
        "PROJECT_ID_VERCEL",
        "TEAM_ID_VERCEL",
        "OPENAI_API_KEY",
        "NEXT_PUBLIC_APP_NAME",
        "NEXT_PUBLIC_APP_SHORT_DOMAIN",
        "NEXT_PUBLIC_APP_DOMAIN",
        "NEXT_PUBLIC_VERCEL_ENV",
        "NEXT_PUBLIC_NGROK_URL",
        "NEXT_PUBLIC_IS_DUB",
        "DATABASE_URL",
        "NEXT_PUBLIC_ROOT_DOMAIN",
        "NOTION_API_KEY",
        "VERCEL_ENV",
        "VERCEL_URL"
      ]
    },
    "lint": {},
    "type-check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "db:push": {},
    "db:generate": {},
    "db:migrate:deploy": {},
    "db:seed": {}
  }
}